@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <div class="d-flex justify-content-center">
        <form method="get">
        <a class="btn btn-success mx-2">Trace</a>
        <a class="btn btn-primary mx-2">Debug</a>
        <a class="btn btn-info mx-2">Information</a>
        <a class="btn btn-warning mx-2">Warning</a>
        <a class="btn btn-danger mx-2">Error</a>
        <a class="btn mx-2 text-light" style="background:purple">Critical</a>
        </form>
    </div>
</div>
